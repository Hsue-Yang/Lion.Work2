@using LionTech.Entity.ERP
@using LionTech.Utility
@using Resources
@model TRAININGAP.Models.Home.UnAuthorizatedModel

@{
    string fullDateTimeNumber = Common.GetDateTimeString();
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=10; IE=9; IE=8; IE=7;" />
    <title>@Resources.Resource.AP_Title</title>
    <link href="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Content/Site.css?@(fullDateTimeNumber)" rel="stylesheet" type="text/css" />
    <link href="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Content/Site.MessageBox.css?@(fullDateTimeNumber)" rel="stylesheet" type="text/css" />
    <link href="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Content/Site.Table.css?@(fullDateTimeNumber)" rel="stylesheet" type="text/css" />
    <link href="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Content/Site.TabStrip.css?@(fullDateTimeNumber)" rel="stylesheet" type="text/css" />
    <link href="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Content/themes/base/jquery.ui.core.css?@(fullDateTimeNumber)" rel="stylesheet" type="text/css" />
    <link href="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Content/themes/base/jquery.ui.theme.css?@(fullDateTimeNumber)" rel="stylesheet" type="text/css" />
    <link href="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Content/jquery.ui.autocomplete.css?@(fullDateTimeNumber)" rel="stylesheet" type="text/css" />
    <script src="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Scripts/jquery-1.5.1.min.js?@(fullDateTimeNumber)" type="text/javascript"></script>
    <script src="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Scripts/jquery.form.js?@(fullDateTimeNumber)" type="text/javascript"></script>
    <script src="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Scripts/jquery.blockUI.js?@(fullDateTimeNumber)" type="text/javascript"></script>
    <script src="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Scripts/jquery-ui-1.8.11.min.js?@(fullDateTimeNumber)" type="text/javascript"></script>
    <script src="@Common.GetEnumDesc(EnumSystemID.ERPAP)/Scripts/LionTechWebERPHelpers.js?@(fullDateTimeNumber)" type="text/javascript"></script>
</head>
<body>

    <table cellspacing="0" cellpadding="0" style="width:100%;">
        <tr>
            <td>
                <div class="layout Header">
                    <table style="width:100%;height:30px;">
                        <tr>
                            <td valign="middle" align="left" class="layout Header Title">@Resources.Resource.AP_Title</td>
                            <td valign="middle" align="right"></td>
                        </tr>
                    </table>
                </div>
                <div class="layout MenuBorder"></div>
            </td>
        </tr>
    </table>

    <div class="layout Main">
        <div class="layout Function">
            <div>
                <table class="layout Function Tool">
                    <tr>
                        <td valign="top" align="center">
                           @if (Model.IsTimeout != null && Model.IsTimeout == true)
                            {
                                <a href="@Common.GetEnumDesc(EnumSystemID.ERPAP)">@Resource.AP_Timeout</a><p />
                            }
                            else
                            { 
                                <a href="javascript:window.history.back();">@Resource.AP_UnAuthorizated</a><p />
                            }
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </div>

</body>
</html>
